{
  "name": "rollup-plugin-napi-image",
  "version": "0.4.1",
  "author": {
    "name": "nemurubaka",
    "url": "https://github.com/cijiugechu"
  },
  "private": false,
  "description": "Rollup plugin for optimizing images powered by @napi-rs/image",
  "keywords": [
    "image",
    "rollup-plugin",
    "rollup",
    "vite",
    "vite-compatible",
    "compression",
    "lossless",
    "jpg",
    "jpeg"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/cijiugechu/rollup-plugin-napi-image",
  "repository": {
    "type": "git",
    "url": "git@github.com:cijiugechu/rollup-plugin-napi-image.git"
  },
  "bugs": "https://github.com/cijiugechu/rollup-plugin-napi-image/issues",
  "scripts": {
    "build-fast": "tsup index.ts --format cjs,esm --dts",
    "build": "pnpm run build-fast -- --dts-resolve",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "rollup": "^3.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vitest": "^0.25.8"
  },
  "dependencies": {
    "@napi-rs/image": "^1.4.4",
    "@rollup/pluginutils": "^5.0.2"
  }
}
